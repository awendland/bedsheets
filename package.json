    "test:e2e": "ava --serial build/tests/e2e/**",
    "test:unit": "ava build/tests/unit/**"
  },
  "dependencies": {
    "googleapis": "^51.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.4",
    "ava": "^3.8.2",
    "prettier": "^2.0.5",
    "shx": "^0.3.2",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3"
  },
  "keywords": [
    "google",
    "sheets",
    "spreadsheet",
    "serverless",
    "faas",
    "rest",
    "database",
    "db"
  ]
}
